<table class="index" id='listing_prodict_exports'>
  <colgroup>
    <col style="width: 50%">
    <col style="width: 10%">
    <col style="width: 10%">
    <col style="width: 10%">
    <col style="width: 10%">
    <col style="width: 10%">
  </colgroup>
  <thead>
  <tr data-hook="admin_shipping_methods_index_headers">
    <th><%= Spree.t(:created_at) %></th>
    <th><%= Spree.t(:total_count) %></th>
    <th><%= Spree.t(:processed_count) %></th>
    <th><%= Spree.t(:progress) %></th>
    <th><%= Spree.t(:export_file) %></th>
    <th data-hook="admin_shipping_methods_index_header_actions" class="actions"></th>
  </tr>
  </thead>
  <tbody>
  <% (@product_exports || []).each do |product_export|%>
      <%= render partial: 'export_row', locals: {product_export: product_export} %>
  <% end %>
  </tbody>
</table>